{
  "name": "conference-hall",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "concurrently -k -c 'yellow,blue' 'npm:dev:*'",
    "dev:web": "tsx ./servers/web.dev.ts",
    "dev:job": "tsx watch ./servers/jobs.ts",
    "start": "node ./build/server/index.js",
    "build": "react-router build",
    "jobs:start": "tsx ./servers/jobs.ts",
    "jobs:build": "echo 'no build needed'",
    "test": "npm run test:db && npm run test:unit",
    "test:db": "NODE_ENV=test prisma db push --accept-data-loss",
    "test:unit": "vitest --browser.headless",
    "test:ui": "vitest --project browser",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui --ui-host=localhost",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset -f && prisma db seed",
    "lint": "oxlint",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "tsc": "react-router typegen && tsc -b",
    "knip": "knip",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@conform-to/zod": "^1.17.1",
    "@dnd-kit/abstract": "^0.3.2",
    "@dnd-kit/dom": "^0.3.2",
    "@dnd-kit/geometry": "^0.3.2",
    "@dnd-kit/react": "^0.3.2",
    "@floating-ui/react": "^0.27.18",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@marsidev/react-turnstile": "^1.4.2",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "@react-email/components": "^1.0.8",
    "@react-router/express": "~7.13.0",
    "@react-router/node": "~7.13.0",
    "@remix-run/form-data-parser": "^0.15.0",
    "@sindresorhus/slugify": "^3.0.0",
    "bullmq": "^5.70.1",
    "class-variance-authority": "^0.7.1",
    "compression": "^1.8.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "firebase": "^12.9.0",
    "firebase-admin": "^13.6.1",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "i18next": "^25.8.13",
    "i18next-browser-languagedetector": "^8.2.1",
    "i18next-fetch-backend": "^7.0.0",
    "ioredis": "^5.9.3",
    "isbot": "^5.1.35",
    "json-2-csv": "^5.5.10",
    "mailgun.js": "^12.7.0",
    "marked": "^17.0.3",
    "morgan": "^1.10.1",
    "nodemailer": "^8.0.1",
    "picocolors": "^1.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-i18next": "^16.5.4",
    "react-router": "~7.13.0",
    "recharts": "^3.7.0",
    "remix-i18next": "^7.4.2",
    "sitemap": "^9.0.0",
    "sonner": "^2.0.7",
    "use-debounce": "^10.1.0",
    "uuid": "^13.0.0",
    "xss": "^1.0.15",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.1.1",
    "@ngneat/falso": "^8.0.2",
    "@playwright/test": "^1.58.2",
    "@react-router/dev": "~7.13.0",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.2.1",
    "@types/compression": "^1.8.1",
    "@types/mdx": "^2.0.13",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.13",
    "@types/nodemailer": "^7.0.11",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitest/browser-playwright": "^4.0.18",
    "concurrently": "^9.2.1",
    "knip": "^5.85.0",
    "lefthook": "^2.1.1",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "prisma": "^7.4.1",
    "tailwindcss": "^4.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18",
    "vitest-browser-react": "^2.0.5"
  }
}
