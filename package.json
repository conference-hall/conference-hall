{
  "name": "conference-hall",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "dotenvx run -f .env.dev -- concurrently -k -c '#fdba74,#93c5fd' 'npm:dev:*'",
    "dev:web": "tsx ./servers/web.dev.ts",
    "dev:job": "tsx watch ./servers/jobs.ts",
    "build": "react-router build",
    "start": "node ./build/server/index.js",
    "jobs:start": "tsx ./servers/jobs.ts",
    "jobs:build": "echo 'no build needed'",
    "test": "npm run test:db && npm run test:unit",
    "test:db": "dotenvx run -f .env.test -- prisma db push --accept-data-loss",
    "test:unit": "vitest --browser.headless",
    "test:ui": "vitest --project browser",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui --ui-host=localhost",
    "db:migrate": "dotenvx run -f .env.dev -- prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:reset": "dotenvx run -f .env.dev -- prisma migrate reset -f",
    "emails": "dotenvx run -f .env.dev -- npx --yes react-email dev --dir ./app/shared/emails/templates --port 3002",
    "lint": "biome check",
    "lint:ci": "biome ci",
    "tsc": "react-router typegen && tsc -b",
    "knip": "knip"
  },
  "dependencies": {
    "@conform-to/zod": "^1.8.2",
    "@dnd-kit/abstract": "^0.1.21",
    "@dnd-kit/dom": "^0.1.21",
    "@dnd-kit/geometry": "^0.1.21",
    "@dnd-kit/react": "^0.1.21",
    "@floating-ui/react": "^0.27.15",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@mjackson/form-data-parser": "^0.9.1",
    "@prisma/client": "^6.14.0",
    "@react-email/components": "^0.5.0",
    "@react-router/express": "~7.8.1",
    "@react-router/node": "~7.8.1",
    "@sindresorhus/slugify": "^2.2.1",
    "bullmq": "^5.58.0",
    "class-variance-authority": "^0.7.1",
    "compression": "^1.8.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "firebase": "^12.1.0",
    "firebase-admin": "^13.4.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "i18next": "^25.3.6",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-fetch-backend": "^6.0.0",
    "ioredis": "^5.7.0",
    "isbot": "^5.1.29",
    "json-2-csv": "^5.5.9",
    "mailgun.js": "^12.0.3",
    "marked": "^16.1.2",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.5",
    "picocolors": "^1.1.1",
    "pretty-cache-header": "^1.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.6.1",
    "react-router": "~7.8.1",
    "recharts": "^3.1.2",
    "remix-i18next": "^7.2.1",
    "sitemap": "^8.0.0",
    "sonner": "^2.0.7",
    "use-debounce": "^10.0.5",
    "uuid": "^11.1.0",
    "xss": "^1.0.15",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@dotenvx/dotenvx": "^1.48.4",
    "@mdx-js/rollup": "^3.1.0",
    "@ngneat/falso": "^8.0.2",
    "@playwright/test": "^1.54.2",
    "@react-router/dev": "~7.8.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.12",
    "@types/compression": "^1.8.1",
    "@types/mdx": "^2.0.13",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.17.2",
    "@types/nodemailer": "^7.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitest/browser": "^3.2.4",
    "concurrently": "^9.2.0",
    "knip": "^5.62.0",
    "lefthook": "^1.12.3",
    "prisma": "^6.14.0",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1.0.1"
  },
  "engines": {
    "node": "22.x"
  }
}
